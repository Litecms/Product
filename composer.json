{
    "name": "litecms/product",
    "description": "Product package for litecms.",
    "keywords": ["lavalite", "Product"],
    "license": "MIT",
    "type": "lavalite-package",
    "authors": [
        {
            "name": "Lavalite",
            "email": "info@lavalite.org"
        }
    ],
    "require": {
       "php": "^7.1.3"
    },
    "autoload": {
        "classmap": [
            "database/seeds"
        ],
        "psr-4": {
            "Litecms\\Product\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Litecms\\Product\\Providers\\ProductServiceProvider"
            ],
            "aliases": {
                "Product"       : "Litecms\\Product\\Facades\\Product"
            }
        }
    },
    "minimum-stability": "dev"
}
